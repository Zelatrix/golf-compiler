var count := 0;
var i := 2;
var number := 1;

while (number <= 100) {
    while(i <= number / 2) {
        if (number mod i == 0) then {
            count := count + 1;
            break;
        };
        i := i + 1;
    };
    
    if (count == 0) and (number != 1) then {
        print(number);
    };
    number := number + 1;
};
